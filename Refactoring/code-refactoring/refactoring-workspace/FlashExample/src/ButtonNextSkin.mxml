<!--- The default skin class for the Spark Button component.  

       @see spark.components.Button
        
      @langversion 3.0
      @playerversion Flash 10
      @playerversion AIR 1.5
      @productversion Flex 4
-->
<s:SparkButtonSkin xmlns:fx="http://ns.adobe.com/mxml/2009" 
             xmlns:s="library://ns.adobe.com/flex/spark" 
             xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
             width="26" height="26"
             alpha.disabled="0.5" xmlns:local="*">
	<fx:Metadata>[HostComponent("ButtonNext")]</fx:Metadata>
     
    <!-- states -->
    <s:states>
        <s:State name="up" />
        <s:State name="over" />
        <s:State name="down" />
        <s:State name="disabled" />
    </s:states>
    
	<local:FlashSymbolContainer includeIn="up">
		<local:ButtonNextUpSymbol />
	</local:FlashSymbolContainer>
	
	<local:FlashSymbolContainer includeIn="over">
		<local:ButtonNextOverSymbol />
	</local:FlashSymbolContainer>
	
	<local:FlashSymbolContainer includeIn="down">
		<local:ButtonNextDownSymbol />
	</local:FlashSymbolContainer>
	
	<local:FlashSymbolContainer includeIn="disabled">
		<local:ButtonNextDisabledSymbol />
	</local:FlashSymbolContainer>
    
</s:SparkButtonSkin>
