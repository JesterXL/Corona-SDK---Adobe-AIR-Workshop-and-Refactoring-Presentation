	public class Lesson3_c_ScrollRect extends Sprite
	{
		
		private var movingMap:MovingMap;
		
		public function Lesson3_c_ScrollRect()
		{
			super();	
			init();
		}
		
		private function init():void
		{
			stage.align = StageAlign.TOP_LEFT;
